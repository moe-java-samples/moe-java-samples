<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.intel.inde.moe.samples.rssreader.android" >

    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.intel.inde.moe.samples.rssreader.android.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.intel.inde.moe.samples.rssreader.android.BookmarksActivity"
            android:label="@string/app_name"
            android:parentActivityName="com.intel.inde.moe.samples.rssreader.android.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.intel.inde.moe.samples.rssreader.android.MainActivity" />
        </activity>

    </application>

</manifest>
